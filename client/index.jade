head
  title Ketchy Ketch

body

template(name="userList")
  .user-list
    ul
      each users
        li
          a(href="/users/{{ _id }}")
            img.avatar(src=profilePhotoUrl)
            span {{ firstName }}

template(name="userDetails")
  .user-details
    h1 {{ firstName }}